<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lanou.a36kedemo">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".tools.MyApp"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".mine.CommentActivity"/>
        <activity android:name=".mine.CollectActivity"/>
        <activity android:name=".mine.UserActivity"/>
        <activity android:name=".mine.SettingsActivity"/>
        <activity android:name=".mine.OffLineActivity" />
        <!--<activity android:name=".mine.PopupPrefix" android:theme="@style/MyDialogStyleBottom"/>-->

    </application>


</manifest>